<Window x:Class="Intent.AnalisisDocumentos.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Intent.AnalisisDocumentos"
        mc:Ignorable="d"
        Title="MainWindow" >
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="50*"/>
            <RowDefinition Height="50*"/>
        </Grid.RowDefinitions>
        <TextBlock Text="Analisis de carpetas" HorizontalAlignment="Center"  VerticalAlignment="Center"/>
        <Button x:Name="button" Content="Cargar documento .csv" HorizontalAlignment="Center"  VerticalAlignment="Top" Width="Auto" Grid.Row="1" Click="button_Click"/>

    </Grid>
</Window>
